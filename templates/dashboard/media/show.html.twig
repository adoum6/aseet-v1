{% extends 'dashboard.html.twig' %}

{% block title %}Media{% endblock %}

{% block body %}
    <h1>Media</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ medium.id }}</td>
            </tr>
            <tr>
                <th>MediaName</th>
                <td>{{ medium.mediaName }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ medium.createdAt ? medium.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ medium.updatedAt ? medium.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ medium.date ? medium.date|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Year</th>
                <td>{{ medium.year }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('activity_media_index') }}">back to list</a>

    <a href="{{ path('activity_media_edit', {'id': medium.id}) }}">edit</a>

    {{ include('activity_media/_delete_form.html.twig') }}
{% endblock %}
